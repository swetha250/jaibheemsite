$(document).mouseup(function(e){
    var container = $("#drop1");
    var droptwo=$("#drop2");
    var dropthree=$("#dropthree");
     var dropfour=$("#dropfour");
     var dropFive=$("#dropFive");
     var dropSix=$("#dropSix");
    if(!container.is(e.target) && container.has(e.target).length === 0){
        container.hide();
    }
     if(!droptwo.is(e.target) && droptwo.has(e.target).length === 0){
        droptwo.hide();
    }
    if(!dropthree.is(e.target) && dropthree.has(e.target).length === 0){
        dropthree.hide();
    }
    if(!dropfour.is(e.target) && dropfour.has(e.target).length === 0){
        dropfour.hide();
    }if(!dropFive.is(e.target) && dropFive.has(e.target).length === 0){
        dropFive.hide();
        
    }if(!dropSix.is(e.target) && dropSix.has(e.target).length === 0){
        dropSix.hide();
    }
});
